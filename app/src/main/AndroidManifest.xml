<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="carlos.com.ticketsapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presentation.splash.InicioActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentation.registro.RegistroActivity"></activity>
        <activity android:name=".presentation.auth.LoginActivity"></activity>
        <activity android:name=".presentation.principal.PrincipalActivity"></activity>
        <activity android:name=".presentation.auth.LoadActivity"></activity>
        <activity android:name=".presentation.profile.ProfileActivity"></activity>
        <activity android:name=".presentation.principal.BottomBar.DetallesMenu.DetallesActivity"></activity>



    </application>

</manifest>